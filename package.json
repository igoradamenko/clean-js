{
  "name": "clean-js",
  "version": "0.1.0",
  "description": "Demo project for Clean JS talk",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --inline --config ./config/webpack.config.js",
    "lint-js": "eslint --cache -c .eslintrc.js src config",
    "lint-scss": "stylelint \"src/**/*.scss\" --syntax scss --cache",
    "test": "mocha src/**/*.test.js"
  },
  "dependencies": {
    "angular": "1.8.0",
    "angular-mocks": "1.5.9",
    "angular-ui-router": "0.2.18"
  },
  "devDependencies": {
    "@funboxteam/eslint-config": "5.0.0",
    "autoprefixer": "9.7.4",
    "chai": "4.2.0",
    "chai-datetime": "1.5.0",
    "css-loader": "3.4.2",
    "csso-webpack-plugin": "2.0.0-beta.1",
    "eslint": "5.16.0",
    "fibers": "4.0.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.1",
    "jade": "1.11.0",
    "jade-loader": "0.8.0",
    "lint-staged": "10.0.7",
    "mocha": "6.2.1",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "resolve-url-loader": "3.1.1",
    "sass": "1.25.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.0.0",
    "stylelint-order": "0.7.0",
    "stylelint-scss": "2.1.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.2"
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix --syntax scss --cache"
    ],
    "*.js": [
      "eslint --fix --cache -c .eslintrc.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -q"
    }
  }
}
