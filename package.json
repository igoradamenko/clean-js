{
  "name": "frontend-refactoring-ru",
  "version": "0.1.0",
  "description": "Frontend Refactoring, Russian version",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --inline --config ./config/webpack.config.js",
    "lint-js": "eslint --cache -c .eslintrc.js src",
    "lint-scss": "stylelint \"src/**/*.scss\" --syntax scss --cache"
  },
  "dependencies": {
    "angular": "1.5.9",
    "angular-ui-router": "0.2.18"
  },
  "devDependencies": {
    "autoprefixer": "9.7.4",
    "css-loader": "3.4.2",
    "csso-webpack-plugin": "2.0.0-beta.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "fibers": "4.0.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.1",
    "jade": "1.11.0",
    "jade-loader": "0.8.0",
    "lint-staged": "10.0.7",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "resolve-url-loader": "3.1.1",
    "sass": "1.25.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "13.0.0",
    "stylelint-order": "0.7.0",
    "stylelint-scss": "2.1.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.2"
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix --syntax scss --cache"
    ],
    "*.js": [
      "eslint --fix --cache -c .eslintrc.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -q"
    }
  }
}
